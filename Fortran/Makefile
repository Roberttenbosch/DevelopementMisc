FC="gfortran"
CC="gcc"
AR="ar"
WRC="windres"
RM=rm -f

IDIR=

LDIR=-L/snap/simplyfortran/4/bin/../lib/ 


OPTFLAGS= -g

SPECIALFLAGS=$(IDIR)

RCFLAGS=-O coff

PRJ_FFLAGS=

PRJ_CFLAGS=

PRJ_LFLAGS=

FFLAGS=$(SPECIALFLAGS) $(OPTFLAGS) $(PRJ_FFLAGS) -Jmodules 

CFLAGS=$(SPECIALFLAGS) $(OPTFLAGS) $(PRJ_CFLAGS)

"build/example.o": "example.f90"
	@echo Compiling example.f90
	@$(FC) -c -o "build/example.o" $(FFLAGS) "example.f90"

clean: .SYMBOLIC
	@echo Deleting build/example.o and related files
	@$(RM) "build/example.o"
	@echo Deleting target
	@$(RM) "target"

"target":  "build/example.o" "build/command-line.prj.target"
	@echo Generating target
	@$(FC) -o "target" -static "build/example.o" $(LDIR) $(PRJ_LFLAGS)

all: "target" .SYMBOLIC

